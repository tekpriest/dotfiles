{
  "codeLens.enable": true,
  "languageserver": {
    "elixirLS": {
      "command": "~/.local/bin/elixir-ls/language_server.sh",
      "filetypes": ["elixir", "eelixir"]
    },
    "golang": {
      "command": "gopls",
      "args": ["serve", "-debug", "0.0.0.0:8484", "-rpc.trace"],
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    },
    "sql": {
      "module": "sql-language-server",
      "args": ["up", "--method", "node-ipc"],
      "filetypes": ["sql"],
    }
  },
  "snippets.ultisnips.pythonPrompt": false,
  "suggest.virtualText": true,
  "coc.preferences.formatOnType": true
}
